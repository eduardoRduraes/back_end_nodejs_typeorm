{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "prettier/@typescript-eslint",
    "plugin:prettier/recommend"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error"
  }
}
